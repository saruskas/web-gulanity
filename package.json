{
  "name": "gulanity-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,json}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "deploy": "npx serve -s out -l 3000",
    "deploy:full": "npm run build && npm run deploy",
    "deploy:npm": "pwsh -ExecutionPolicy Bypass -File ./deploy-npm.ps1",
    "deploy:docker": "pwsh -ExecutionPolicy Bypass -File ./deploy.ps1",
    "deploy:docker:quick": "pwsh -ExecutionPolicy Bypass -File ./deploy.ps1 -SkipBuild"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.7",
    "@hookform/resolvers": "^5.2.2",
    "@next/third-parties": "^16.0.3",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@types/js-yaml": "^4.0.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.24",
    "js-yaml": "^4.1.1",
    "lucide-react": "^0.546.0",
    "next": "15.4.2",
    "ogl": "^1.0.11",
    "qr-code-styling": "^1.9.2",
    "qrcode.react": "^4.2.0",
    "react": "19.1.0",
    "react-cookie-consent": "^9.0.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.65.0",
    "react-qr-code": "^2.0.18",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.4.2",
    "jsdom": "^27.2.0",
    "postcss": "^8.5.6",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5",
    "vitest": "^4.0.10"
  }
}
